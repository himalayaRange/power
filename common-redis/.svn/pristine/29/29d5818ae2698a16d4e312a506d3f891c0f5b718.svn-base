#spring-session + redis实现分布式回话session同步
#redis.host=127.0.0.1
#本地虚拟机
#redis.host=192.168.113.128
redis.host=192.168.12.42
redis.port=6379
redis.password=bly@xxb_o2o_myRedis
redis.db=10
#连接超时时间ms
redis.timeout=10000
#session活动最长时间s
redis.maxInactiveIntervalInSeconds=1800
#设置最大连接数（根据并发请求合理设置）
redis.maxTotal=100
#设置最大空闲数（根据并发请求合理设置）
redis.maxIdle=25
#设置最小空闲数（初始化连接数）
redis.minIdle=10
#最大等待时间ms
redis.maxWaitMillis=10000